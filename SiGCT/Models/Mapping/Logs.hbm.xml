<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Integration.Core"
                   namespace="Integration.Core.Model" >

  <class name="Logs">

    <id name="Id" column="Id" type="">
      <generator class="native" />
    </id>

    <property name="Date" column="Date" />
    <property name="Thread" column="Thread" length="255" />
    <property name="Level" column="Level" length="50" />
    <property name="Logger" column="Logger" length="255"  />
    <property name="Message" column="Message" length="4000"  />
    <property name="Exception" column="Exception" length="2000"  />

  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Integration.Core"
                   namespace="Integration.Core.Model" >

	<class name="Titulo">
		<meta attribute="class-description">Titulos</meta>

		<id name="Id">
			<meta attribute="field-description">
				Identificação unica do Titulo
			</meta>
			<generator class="native" />
		</id>

		<property name="DataEmissao" column="DataEmissao" type="Date" />

		<many-to-one name="Empresa" class="Empresa" column="IdEmpresa" cascade="none" />
		<many-to-one name="Cliente" class="Cliente" column="IdCliente" cascade="none" />
		<many-to-one name="Canal" class="Canal" column="IdCanal" cascade="none" />

    <property name="DataAlteracao" />
    <property name="Ativo" />

	</class>
</hibernate-mapping>

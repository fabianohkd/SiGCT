<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Integration.Core"
                   namespace="Integration.Core.Model" >

	<class name="Terminal">
		<meta attribute="class-description">Terminais de venda on-line, vendas com POS</meta>

		<id name="Id">
			<generator class="native" />
		</id>

		<property name="Codigo" length="100" />
		<property name="NumeroSerie" length="80" />

    <property name="DataCadastro" type="Date" />
    <property name="DataNegociacao" />
    <property name="Credenciamento" />
    <property name="UltimaTransacao" />

    <bag name="Vendas" lazy="true" mutable="false" access="" cascade="none" >
      <key column="IdTerminal" />
      <one-to-many class="Venda"  />
    </bag>
    
		<many-to-one name="Cliente" class="Cliente" column="IdCliente" />
    <many-to-one name="Canal" class="Canal" column="IdCanal" />

    <property name="DataAlteracao" />
    <property name="Ativo" />
		
	</class>
</hibernate-mapping>

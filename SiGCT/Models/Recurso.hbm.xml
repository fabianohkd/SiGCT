<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SiGCT"
                   namespace="SiGCT.Models" >

	<class name="RotaCliente">
		<meta attribute="class-description">RotasCliente dos representantes</meta>

		<id name="Id">
			<meta attribute="field-description">Identificação unica da rotacliente</meta>
			<generator class="native" />
		</id>

		<property name="Ordem" />

		<many-to-one name="Rota" class="Rota" column="IdRota" cascade="none" />
    <many-to-one name="Cliente" class="Cliente" column="IdCliente" cascade="none" />

    <property name="DataAlteracao" />
    
	</class>
</hibernate-mapping>

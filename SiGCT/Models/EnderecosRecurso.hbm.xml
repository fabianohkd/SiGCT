<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Integration.Core"
                   namespace="Integration.Core.Model" >

	<class name="Fabricante" table="Fabricante">

		<id name="Id" >
			<generator class="native" />
		</id>

    <property name="Nome" length="50" />
    
		<bag name="Modelos" lazy="false" mutable="false" access="" cascade="all" >
			<key column="IdFabricante" />
			<one-to-many class="Modelo" />
		</bag>

    <property name="DataAlteracao" />
    <property name="Ativo" />
		
	</class>
</hibernate-mapping>
